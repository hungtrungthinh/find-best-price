	<div id="list" class="container">
<%if(data){ %>
	<%data.forEach(e=>{%>
<div id="item" class="row">
		<div class="col-md-1 col-xs-4 col-sm-2"><img class="img-thumb" src="<%=e.product.thumbnail_url%>"/></div>
		<div class="col-md-11  col-xs-8 col-sm-10">
				<div class="info">
			<div class="row"><p class="title"><img class="icon-brand" src="images/icon.png"/><a href="https://tiki.vn/<%=e.product.url_path%>" target="_blank"><%=e.product.name%></a><span class="price-sale"> <script>document.write(priceToVnd(<%=e.product.price%>));</script></span></p> </div>
			<div class="row"><span class="price-stock"><s><script>document.write(priceToVnd(<%=e.product.list_price%>));console.log(priceToVnd(<%=e.product.list_price%>));</script></s> <span class="price-down">-<%=e.discount_percent%>%</span><a href="https://tiki.vn/<%=e.product.url_path%>" target="_blank"><button type="button" class="btn btn-primary btn-pay"><i class="fas fa-shopping-cart"></i> Mua ngay</button></a></span></div>
		 
			</div>
			
		</div>
				
</div>
<% }) %>
<%}%>
 
